								{
									"listen": "test",
									"script": {
										"exec": [
											"//User Name generated\r",
											"let userName = pm.response.json();\r",
											"let newUser = userName.replace(\"User \", \"\").replace(\" has been created\",\"\")\r",
											"pm.environment.set(\"NewUser\" , newUser)\r",
											"\r",
											"\r",
											"\r",
											"pm.test(\"Status code is 200\", function () {\r",
											"    pm.response.to.have.status(200);\r",
											"});\r",
											"pm.test(\"Body matches string\", function () {\r",
											"    pm.expect(pm.response.text()).to.include(\"has been created\");\r",
											"});\r",
											"\r",
											""
										],
										"type": "text/javascript"
									}
								},
								{
									"listen": "prerequest",
									"script": {
										"exec": [
											"function generateRandomIntegerInRange(min, max) {\r",
											"    return Math.floor(Math.random() * (max - min + 1)) + min;\r",
											"}\r",
											"\r",
											"let val = generateRandomIntegerInRange(8000000000, 9000000000);\r",
											"\r",
											"pm.environment.set(\"mobileNumber\" , val)\r",
											"\r",
											"\r",
											"\r",
